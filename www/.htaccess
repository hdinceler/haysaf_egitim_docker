<IfModule mod_expires.c>
  ExpiresActive On
  ExpiresByType text/css "access plus 1 year"
</IfModule>

<IfModule mod_headers.c>
  <FilesMatch "\.(css|js)$">
    Header set Cache-Control "public, max-age=31536000, immutable"
  </FilesMatch>
</IfModule>
<IfModule mod_deflate.c>
  AddOutputFilterByType DEFLATE text/css text/html text/javascript application/javascript
</IfModule>

RewriteEngine On

# Ana dizin
RewriteBase /

# Gerçek dosya veya klasör varsa dokunma
RewriteCond %{REQUEST_FILENAME} -f [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^ - [L]

# SEF URL'leri index.php'ye yönlendir


RewriteRule ^login$ index.php?job=login [L,QSA]
 
RewriteRule ^([a-z0-9-]+)/?$ index.php?job=$1 [L,QSA]

